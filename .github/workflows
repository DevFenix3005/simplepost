name: Java Springboot CI

on: [push]

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v2
      - name: Set up JDK 11.04
        uses: actions/setup-java@v1
        with:
          java-version: 11.04
      - name: Build with Gradle
        run: ./gradlew build